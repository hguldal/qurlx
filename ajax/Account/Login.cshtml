@{
 
   var ePosta=Request["txtEposta"];
   var parola=Request["txtParola"];  
    
    try 
    {
        if (WebSecurity.Login(ePosta,parola))
        {
            Response.Write("OK");
            Response.End();
        }
        else 
        {
            Response.StatusCode=400;
            Response.End();
        }
    }
    
    catch (Exception e)
    {
        Response.Write(e.Message);
        Response.End();
    }

    
}
