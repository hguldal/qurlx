@{

if (!WebSecurity.IsAuthenticated)
   
    {
        Response.StatusCode = 401;
        Response.End();
                   
    }  
var GrupID=Request["GrupID"];

var db=Database.Open("veritabani");

if (db.QueryValue("SELECT COUNT(*) FROM Grup_SikayetEdilenGrup WHERE GrupID=@0 AND SikayetEdenUserID=@1",GrupID,WebSecurity.CurrentUserId)>0)
{
    Response.Write("zatensikayetetmis");
    Response.End();
}


db.Execute("INSERT INTO Grup_SikayetEdilenGrup(GrupID,SikayetEdenUserID,SikayetTarihi) VALUES (@0,@1,@2)",GrupID,WebSecurity.CurrentUserId,DateTime.Now);

if (db.QueryValue("SELECT COUNT(*) FROM Grup_SikayetEdilenGrup WHERE GrupID=@0",GrupID)>5)
{
    db.Execute("INSERT INTO Grup_YasakliGrup(GrupID,YasaklanmaTarihi) VALUES (@0,@1)",GrupID,DateTime.Now);    
    db.Execute("DELETE FROM Grup_SikayetEdilenGrup WHERE GrupID=@0",GrupID);
}

db.Close();
 Response.StatusCode = 200;
 Response.End();        
}
