@{

if (!WebSecurity.IsAuthenticated)
   
        {
            Response.StatusCode=401;
            Response.End();
                   
        }      

    var PaylasimID=Request["PaylasimID"];
    var db=Database.Open("veritabani");
    
    var queryKisiler=db.Query("SELECT * FROM Paylasim_Begeni INNER JOIN Profiles ON Paylasim_Begeni.UserID=Profiles.UserID WHERE Paylasim_Begeni.PaylasimID=@0",PaylasimID);

    Json.Write(queryKisiler, Response.Output);
    db.Close();   
    Response.ContentType = "application/json";
    Response.End();

    
}

